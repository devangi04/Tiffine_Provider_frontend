{
  "expo": {
    "name": "Tiffine Provider",
    "slug": "tiffinprovider",
    "version": "1.0.0",

    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "tiffine-provider",
    "jsEngine": "hermes",
    "android": {
      "package": "com.triospheretech.tiffineprovider",
      "versionCode": 1,

      "statusBar": {
    "translucent": true,
    "backgroundColor": "#00000000",
    "barStyle": "light-content"
  },
      "enableProguardInReleaseBuilds":true,
      "enableMinifyInReleaseBuilds":true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash2.png",
        "backgroundImage": "./assets/images/splash2.png",
        "backgroundColor": "#0C831F"
      },
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "POST_NOTIFICATIONS"
      ]
    },
    "ios": {
      "bundleIdentifier": "com.triospheretech.tiffineprovider",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "userInterfaceStyle": "automatic",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "New Notification"
        }
      ],
  [
  "expo-build-properties",
  {
    "android": {
      "compileSdkVersion": 35,
      "targetSdkVersion": 34,
      "buildToolsVersion": "35.0.0",
      "enableProguardInReleaseBuilds": true,
      "enableMinifyInReleaseBuilds": true,
      "useLegacyPackaging": false
    },
    "ios": {
      "deploymentTarget": "15.1"
    }
  }
]

    ],
    "owner": "triosphere",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3839ea0b-a96e-4103-9f31-5be96e40af52"
      }
    }
  }
}
